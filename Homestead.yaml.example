ip: 192.168.10.11
memory: 2048
cpus: 2
provider: virtualbox
authorize: ~/.ssh/id_rsa.pub
keys:
    - ~/.ssh/id_rsa
folders:
    -
        map: .
        to: /home/vagrant/beauty
        type: "nfs"
sites:
    -
        map: beauty.test
        to: /home/vagrant/beauty/public
        type: symfony4
        php: "7.3"

databases:
    - beauty
features:
    -
        mariadb: false
    -
        ohmyzsh: false
    -
        webdriver: false
name: beauty
hostname: beauty
